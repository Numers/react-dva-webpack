{
  "presets": [
    "react",
    "es2015",
    "es2016",
    "stage-1",
  ],
  "plugins": [
    "transform-decorators-legacy" ,
    "transform-runtime",
    ["import", { "libraryName": "antd-mobile", "style": "css"}]
  ],

  "env": {
    "development": {
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]]
    }
  }
}
